   "source": [
    "# array with discrete values\n",
    "x = np.linspace(0,4, int(10e5))\n",
    "# calculate constant C\n",
    "C = 1 / x.sum()\n",
    "# create function fx\n",
    "fx = np.power(x, 2)\n",
    "# calculate probability px\n",
    "px = x * C\n",
    "# calculate expectational value E\n",
    "E = np.sum((fx * px))\n",
    "print(E)"
   ]
